extends layout 

block content 
  #gamelist-title-container
    h1 Publishers 
    a(href="/publisher/create") #[button Add a Publisher]
  #gamelist-container
    for publisher in data 
      .gamelist-game
        a(href=publisher.url)
          img(src=`/images/uploads/${publisher.image}`, alt="Publisher logo")
          p=publisher.name 